{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "next",
        "eslint:recommended"
        // "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off"
    }
}
